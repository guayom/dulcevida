<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="/images/favicon.ico" type="image/ico" sizes="16x16">
    <title><%= current_page.data.title || "La Dulca vida at Hacienda Pinilla" %></title>
    <%= stylesheet_link_tag "site" %>
  </head>

  <body>
    <%= partial "partials/general/bar" %>
    <%= partial "partials/general/header" %>
    <%= yield %>
    <%= partial "partials/general/footer" %>
    <%= partial "partials/general/back_to_top" %>
    <%= javascript_include_tag "jquery-3.2.1.min" %>
    <%= javascript_include_tag "bootstrap.min" %>
    <%= javascript_include_tag "site" %>
  </body>
</html>
